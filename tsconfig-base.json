{
  "compilerOptions": {
    "experimentalDecorators": true,
    "baseUrl": "./",
    "paths": {
      "@/atomic/*": ["src/component/atomic/*"],
      "@/molecules/*": ["src/component/molecules/*"],
      "@/organism/*": ["src/component/organism/*"],
      "@/helper/*": ["src/utils/helper/*"],
      "@/modules/*": ["src/utils/modules/*"],
      "@/constant/*": ["src/utils/constant/*"],
      "@/interface/*": ["src/utils/interface/*"],
      "@/assets/*": ["src/assets/*"],
      "@/utils/*": ["src/utils/*"],
      "@/style/*": ["src/style/*"],
      "@/*": ["src/*"],
    }
  }
}
